<div class="container">
  <h1 class="text-center">Your FAVorite Pokemon!</h1>
  <p class="text-center">
    <a href="/">All Pokemon</a>
  </p>

  <% pokemons.forEach(function(pokemon) { %>
    <div class="well">
      <h2><%= pokemon.name %></h2>
      <form method="POST" action="/show">
        <input hidden type="text" name="name" value="<%= pokemon.name %>">
        <button class="btn btn-primary" type="submit">Look at Me</button>
      </form>
    </div>
  <% }); %>
</div>


